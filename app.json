{
  "expo": {
    "name": "ToDo",
    "slug": "todo-firebase",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.chiefbark.todo",
      "versionCode": 5
    },
    "ios": {
      "supportsTablet": true
    },
    "description": "Simple todo's application using firebase",
    "githubUrl": "https://github.com/Chiefbark/RN-ToDo-s"
  }
}
